[user]
	name = 'Ruben Sierra'
	email = ''
[alias]
	co = checkout
[color]
	diff = auto
	status = auto
	branch = auto
[core]
	excludesfile = <%= ENV['HOME'] %>/.gitignore
	editor = subl -w
	autocrlf = input
[apply]
	whitespace = nowarn
[format]
	pretty = %C(yellow)%h%Creset %s %C(red)(%an, %cr)%Creset
[github]
	user = 'rsierra'
	token = ''
[merge]
    ff = false
[branch]
    autosetuprebase = always
[push]
	default = simple
